{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SocialMediaDbContext": "UseGitHubSecret:SMA_DB_CONNECTION_STRING"
  },
  "Authentication": {
    "JwtKey": "UseGitHubSecret:JWT_KEY",
    "JwtExpireDays": "UseGitHubSecret:JWT_EXPIRE_DAYS",
    "JwtIssuer": "UseGitHubSecret:JWT_ISSUER"
  },
  "Azure": {
    "StorageConnectionString": "UseGitHubSecret:SMA_STORAGE_CONNECTION_STRING",
    "BlobContainerName": "UseGitHubSecret:SMA_BLOB_NAME"
  }
}
